<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:styles="styles.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.SkinnableContainer")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<styles:GraphicsLib id="graphicsLib"/>
	
	<s:Rect top="0" bottom="0" right="0" left="0" filters="{[graphicsLib.getDropShadowFilterWhite()]}">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:GradientEntry color="0xffffff"/>
				<s:GradientEntry color="0xeeeeee"/>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
	
	<s:Group id="contentGroup" left="10" right="10" top="10" bottom="10"/>
	
	<!-- SkinParts
	name=contentGroup, type=spark.components.Group, required=false
	-->
</s:Skin>
